+---------------------------
  Deni Fibonacci function
---------------------------+
#driver "fi"
-- declare Fibonacci function
define fib(n ∈ N) => (y ∈ N): 
  do if (n = 1 ∨ n = 0):
    modify y := 1;
  else
    modify y := fib(n - 1) + fib(n - 2);
  done;
define;
-- call define fib 
create r := fib(5):
-- print the result
print r;
over;