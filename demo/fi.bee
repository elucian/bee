+---------------------------
  Declare Fibonacci define
---------------------------+
#driver "fi"
-- declare Fibonacci define
define fib(n ∈ N) => (y ∈ N): 
  when (n = 1 ∨ n = 0):
    modify y := 1;
  else:
    modify y := fib(n - 1) + fib(n - 2);
  when;
define;
-- call define fib 
create r := fib(5):
-- print the result
print r;
over;