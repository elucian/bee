+---------------------------
  Declare Fibonacci function
---------------------------+
#driver "fi"

-- declare Fibonacci function
aspect fib(n ∈ N) => (y ∈ N): 
  when (n = 1 ∨ n = 0):
    alter y := 1;
  else:
    alter y := fib(n - 1) + fib(n - 2);
  when;
aspect;

-- call function fib 
value r := fib(5):

-- print the result
print r;


over;