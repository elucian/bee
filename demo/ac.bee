+---------------------------------
  asynchronous call using "start" 
---------------------------------+
#driver "ac"

-- suspend for 2.5 sec
rule test():
  wait 2.5;
rule;

-- start 4 threads
make i := 0; -- control variable
while (i â‰¤ 4):
  start test;    
  alter i += 1;    
while;
rest;

over.