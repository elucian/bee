+-----------------------------------+
#name :=  "ac";

**  suspend for 2.5 sec
rule test():
  wait 2.5 
return; ** test

**  start 4 threads
make i := 0 ; **  control variable
while (i â‰¤ 4)
  start test
  alter i += 1
repeat;
rest; ** synchronize

over.
