+--------------------------
 Switch statement demo
---------------------------+
#driver "sw"

make msg @ S;
make var âˆˆ Z;
read(var,"var:>");
switch var:
  is 0:
    alter msg := "zero";
  is 1:
    alter msg := "one";
  in (1,3,5,7,9):
    alter msg := "odd";
  in [1..3]:
    alter msg := "small";
  in [4..9]:
    msg := "large";
other:
  msg := "Undexpected";
switch;

print msg;

over;