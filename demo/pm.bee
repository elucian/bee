+-------------------------------
  pattern matching rule
------------------------------+
#name :=  "pm";

-- pattern matching expression
rule pm(p1,p2 ∈ N) ∈ N =>
  (0 ? (p1 = 0 )
  ,1 ? (p2 = 0 ) 
  ,2 ? (p1 = p2) 
  ,3 ? (p1 > p2)
  ,4 ? True );
-- execute pm expression
print pm(0,2); --0
print pm(1,0); --1
print pm(1,1); --2
print pm(1,2); --3
print pm(2,1); --4

over.

