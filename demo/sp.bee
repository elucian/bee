driver sp:
load $pro_lib.console.(*);

rule swap(x @ Z, y @ Z):
   ** create temporary value
   make  t := x; 
   
   ** swap the values in a single statement
   alter y := x, x := t; 
return;

rule main():
   make a := 1, b := 2;
   print "a = #(n)  b = (n)" ? (a,b);
   apply swap(a, b); 
   print "a = #(n) b = ##n) #(n)" ? (a, b);
return;
over.