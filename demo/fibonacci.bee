load $pro_lib.console:(*);
** declare Fibonacci method
method fib(n ∈ N) => (y ∈ N):
  when (n = 1) ∨ (n = 0) do
    alter y := 1; //first value
  else     
    alter y := fib(n-1) + fib(n-2);
  done;
return;

method main():
   ** call fib method using argument 5
   make r := fib(n:5);
   
   ** print the result to console
   print r;
   #end of module //end driver fi
return;